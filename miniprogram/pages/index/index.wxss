.image {
  border: solid 1rpx #141b77;
  border-radius: 20rpx;
}
.column-btn {
  display: flex;
  justify-content: space-around;
}
.column-btn .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  color: #141b77;
  font-size: 32rpx;
  border: solid 1rpx #141b77;
  border-radius: 10rpx;
}